
public class CaixaEletronico {
	public static void main(String[] args) {
		int nota100, nota50, nota20, nota10, nota5, nota2, nota1;
		int saque = 188;
		int resto;

		nota100 = saque / 100;
		resto = saque % 100;
		nota50 = resto / 50;
		resto= resto % 50;
		nota20 = resto / 20;
		resto= resto % 20;
		nota10 = resto / 10;
		resto= resto % 10;
		nota5 = resto / 5;
		resto= resto % 5;
		nota2 = resto / 2;
		resto= resto % 2;
		nota1 = resto / 1;
		resto= resto % 1;

		System.out.println("Sair√° do caixa " + nota100 + " de R$100");
		System.out.println(nota50);
		System.out.println(nota20);
		System.out.println(nota10);
		System.out.println(nota5);
		System.out.println(nota2);
		System.out.println(nota1);

	}

}
